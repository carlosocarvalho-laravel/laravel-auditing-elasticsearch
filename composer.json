{
    "name": "carlosocarvalho/laravel-auditing-es",
    "description": "A elasticsearch driver for the owen-it/laravel-auditing package. Allows storage of the audits in elasticsearch.",
    "keywords": [
        "accountability",
        "audit",
        "auditing",
        "changes",
        "eloquent",
        "history",
        "log",
        "logging",
        "observer",
        "laravel",
        "lumen",
        "record",
        "revision",
        "tracking",
        "elasticsearch",
        "es"
    ],
    "type": "package",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/carlosocarvalho/laravel-auditing-es/issues",
        "source": "https://github.com/carlosocarvalho/laravel-auditing-es"
    },
    "authors": [
        {
            "name": "Carlos Carvalho",
            "email": "contato@carlosocarvalho.com.br"
        }
    ],
    "require": {
        "php": ">=7.0",
        "owen-it/laravel-auditing": "^7.0",
        "elasticsearch/elasticsearch": "6.*"
    },
    "autoload": {
        "psr-4": {
            "CarlosOCarvalho\\Auditing\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarlosOCarvalho\\Auditing\\ElasticSearchAuditingServiceProvider"
            ]
        }
    }
}
